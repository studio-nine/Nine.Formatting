{
  "version": "1.5.0-*",
  "description": "A portable text and binary serialization library for flat POCOs",
  "authors": [ "Yufei Huang" ],
  "tags": [ "PCL", "Portable", "Serialization", "Formatting", "Json", "Protobuf" ],
  "iconUrl": "https://avatars3.githubusercontent.com/u/9212229?s=64",
  "projectUrl": "https://github.com/studio-nine/Nine.Formatting",
  "licenseUrl": "https://github.com/studio-nine/Nine.Formatting/blob/master/LICENSE",

  "dependencies": {
    "Newtonsoft.Json": "8.0.2",
    "Nine.Formatting.Abstractions": "1.5.0-*"
  },

  "frameworks": {
    "dotnet5.0": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.0"
      },
      "imports": "portable-net45+wp80+win8+wpa81+dnxcore50"
    },
    "dnx451": { 
        "compilationOptions": {
            "define": [ "FEATURE_JIL" ]
        },
        "dependencies": {
            "Jil": "2.13.0"
        }
    },
    "net451": {
        "compilationOptions": {
            "define": [ "FEATURE_JIL" ]
        },
        "dependencies": {
            "Jil": "2.13.0"
        }
    },
    ".NETPortable,Version=v4.5,Profile=Profile7": {
      "frameworkAssemblies": {
        "Microsoft.CSharp": "",
        "System": "",
        "System.Collections": "",
        "System.Core": "",
        "System.Diagnostics.Debug": "",
        "System.Globalization": "",
        "System.IO": "",
        "System.Linq": "",
        "System.Reflection": "",
        "System.Reflection.Extensions": "",
        "System.Runtime": "",
        "System.Runtime.Extensions": "",
        "System.Text.Encoding": "",
        "System.Text.Encoding.Extensions": "",
        "System.Text.RegularExpressions": "",
        "System.Threading": "",
        "System.Threading.Tasks": ""
      }
    }
  }
}
